filetype plugin on
syntax on

set t_Co=256
set background=dark

let base16colorspace=256
colorscheme zenburn

set backspace=indent,eol,start
set complete-=i
set smarttab
set incsearch
set tabstop=2
set shiftwidth=2
set backspace=2
set encoding=utf-8
set number
set hlsearch
set ignorecase
set smartcase
